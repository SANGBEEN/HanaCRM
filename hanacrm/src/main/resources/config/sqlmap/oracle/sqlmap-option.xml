<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="option.OptionDAO">
	<insert id="insert" parameterType="option">
		insert into deposit_options values(
			deposit_options_seq.nextval, #{dclsMonth},#{finCoNo},#{finPrdtCd},#{intrRateType},#{intrRateTypeNm},
			#{saveTrm},#{intrRate},#{intrRate2}
		)
	</insert>
</mapper>