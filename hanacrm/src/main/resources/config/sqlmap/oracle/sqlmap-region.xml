<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="region.RegionDAO">

<!-- 쿼리 -->
	<select id="select" resultType="region">
		select * from region
	</select>
	<select id="selectByNo" parameterType="int" resultType="region">
		select * from region where no=#{no}
	</select>
	
	
</mapper>
